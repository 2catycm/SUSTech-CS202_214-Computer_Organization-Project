.data
	array: .space 40
.text
    li $t0 0
    li $t1 1
    li $t2 2
    li $t3 3
    li $t4 4
    la $s0 array
    sw $t4 0($s0)
    sw $t2 4($s0)
    sw $t3 8($s0)
    sw $t1 12($s0)
    sw $t0 16($s0)
    move $v0 $s0
    addi $v1 $v0 20
    move $a0 $v1
    # jal copy
    jal to_signed_array
    
    addi $v0 $s0 20
    addi $v1 $v0 20
    jal insertion_sort

   addi $v0 $s0 20
   addi $v1 $v0 20
   jal max
    li $v0 1
   syscall
   addi $v0 $s0 20
   addi $v1 $v0 20
   jal min
    li $v0 1
   syscall
   
   li $v0 10
   syscall
    
.include "../commons/std_algorithm.mips"
